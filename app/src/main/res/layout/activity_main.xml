    <Button
        android:id="@+id/btnclr"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="0dp"
        android:layout_height="86dp"
        android:layout_marginEnd="1dp"
        android:backgroundTint="#C5C4BC"
        android:shadowColor="#C5C4BC"
        android:text="@string/btn_clearone"
        android:textColor="#070707"
        android:textSize="16sp"
        android:onClick="clearOne"
        app:layout_constraintBaseline_toBaselineOf="@+id/btnequal"
        app:layout_constraintEnd_toStartOf="@+id/btnequal"
        app:layout_constraintStart_toEndOf="@+id/btn0" />

    <Button
        android:id="@+id/btnequal"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:backgroundTint="@color/orange"
        android:shadowColor="#C5C4BC"
        android:text="@string/btn_equal"
        android:textColor="#070707"
        android:textSize="30sp"
        android:onClick="evaluate"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnclr"
        app:layout_constraintTop_toBottomOf="@+id/btn1" />

